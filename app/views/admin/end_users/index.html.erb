<main class="posts">
 <div class="posts_container">
  <div class="posts_main">
    <h3>会員一覧</h3>
      <table>
       <thead>
        <tr>
         <th>会員ID</th>
         <th>名前</th>
         <th>メールアドレス</th>
         <th>ステータス</th>
        </tr>
       </thead>
       <div class="scroll-list jscroll">
       <tbody>
        <% @end_users.each do |end_user| %>
        <tr>
          <td>
           <%= link_to(edit_admin_end_user_path(end_user.id)) do%>
            <%= end_user.id %>
           <% end %>
          </td>
         <td>
          <%= link_to(edit_admin_end_user_path(end_user.id)) do%>
           <%= end_user.name %>
          <% end %>
         </td>
         <td><%= end_user.email %></td>
         <td>
           <% if end_user.is_active == true %>
            <span class="text-success text">有効</span>
           <% else end_user.is_active == false %>
            <span>退会済</span>
           <% end %>
         </td>
        </tr>
       <% end %>
      </tbody>
     </table>
    </div>
  </div>
 </div>
</main>
