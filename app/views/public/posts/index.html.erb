<% @posts.each do |post| %>
<div>
　<%= link_to public_post_path(post.id) do%>
  　<%= attachment_image_tag post, :image, format: 'jpeg', fallback: "no_image.jpg", size: "160x160"%>
  <% end%>
  <p>ユーザーネーム：<%= post.end_user.name %></p>
  <p><%= post.body %></p>
  <p>投稿日時：<%= post.created_at.strftime('%Y/%m/%d') %></p>
</div>
<% end %>