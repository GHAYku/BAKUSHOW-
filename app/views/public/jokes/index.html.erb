<% @jokes.each do |joke| %>
<div>
　<%= link_to public_joke_path(joke.id) do%>
  　<%= attachment_image_tag joke, :image, format: 'jpeg', fallback: "no_image.jpg", size: "160x160"%>
  <% end%>
  <p>ユーザーネーム：<%= joke.end_user.name %></p>
  <p><%= joke.body %></p>
  <p>投稿日時：<%= joke.created_at.strftime('%Y/%m/%d') %></p>
</div>
<% end %>