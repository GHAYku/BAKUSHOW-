<% @posts.each do |post| %>
 <% if post.title_id.present? %>
  <div>
   <label>お題</label>
   <%= post.title.body%>
  </div>
 <% end%>
<div>
  <%= attachment_image_tag post, :image %>
  <p>ユーザーネーム：<%= post.end_user.name %></p>
  <p><%= post.body %></p>
  <p>投稿日時：<%= post.created_at.strftime('%Y/%m/%d') %></p>
</div>
<% end %>
<%= link_to "編集する", edit_public_end_user_path(current_end_user.id)%>