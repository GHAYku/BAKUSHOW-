<div class="form_container">
 <div class="form_group">
  <h2 class="form_title">ログイン</h2>
   <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field">
      <%= f.label :"メールアドレス" %><br />
     <div>
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
     </divclass="field">
    </div>
    <div class="field">
     <%= f.label :"パスワード" %><br />
     <%= f.password_field :password, autocomplete: "current-password" %>
    </div>

   <% if devise_mapping.rememberable? %>
    <div class="field">
     <%= f.check_box :remember_me %>
     <%= f.label :remember_me %>
    </div>
   <% end %>
   <div class="actions">
    <%= f.submit "Log in" %>
   </div>
  <% end %>
 </div>
</div>
<%= link_to "Twitterでログイン", end_user_twitter_omniauth_authorize_path, method: :post%>
<%= link_to "googleでログイン", end_user_google_oauth2_omniauth_authorize_path, method: :post%>
<%= link_to "Fecebookでログイン", end_user_facebook_omniauth_authorize_path, method: :post%>
<%= render "devise/shared/links" %>
